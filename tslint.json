{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "interface-name" : [true, "never-prefix"],
        "object-literal-sort-keys": false
    },
    "jsRules": {},
    "rulesDirectory": []
}